  <string name="mozac_browser_toolbar_long_press_popup_paste_and_go">Incullà è apre</string>
  <!-- Text for the title displayed in the contextual feature recommendation popup promoting the new toolbar redesign. -->
  <string name="mozac_toolbar_cfr_title">Cuncepitu per Android. Perfezziunatu per voi</string>
