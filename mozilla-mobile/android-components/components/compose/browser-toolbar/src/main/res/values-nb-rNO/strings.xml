<resources>
  <!-- Content description for the clear URL text button. -->
  <string name="mozac_clear_button_description">Tøm</string>
  <!-- Placeholder text shown in the search bar before a user enters text for the default engine -->
  <string name="mozac_browser_toolbar_search_hint">Søk eller skriv inn adresse</string>
  <!-- Contextual menu option for long pressing on the toolbar URL. Clicking it will copy the current URL -->
  <string name="mozac_browser_toolbar_long_press_popup_copy">Kopier</string>
  <!-- Contextual menu option for long pressing on the toolbar URL. Clicking it will paste the text from clipboard and allow user to edit it -->
  <string name="mozac_browser_toolbar_long_press_popup_paste">Lim inn</string>
