  <string name="mozac_browser_toolbar_long_press_popup_paste_and_go">Einfügen &amp; Los</string>
  <!-- Text for the title displayed in the contextual feature recommendation popup promoting the new toolbar redesign. -->
  <string name="mozac_toolbar_cfr_title">Entwickelt für Android. Für Sie verbessert</string>
  <!-- Text for the description displayed in the contextual feature recommendation popup promoting the new toolbar redesign. -->
  <string name="mozac_toolbar_cfr_description">Wir haben das Design mit einem schlanken, modernen Touch aktualisiert</string>
