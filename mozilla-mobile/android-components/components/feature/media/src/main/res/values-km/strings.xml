  <string name="mozac_feature_media_notification_action_pause">ផ្អាក</string>
  <!-- Neutral title of the media notification for when a website that is open in private mode. -->
  <string name="mozac_feature_media_notification_private_mode">គេហទំព័រមួយកំពុងចាក់មេឌៀ</string>
