<resources>
  <!--
    Message announced to the user when tab tray is selected
    The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_tab_counter_open_tab_tray">التبويبات المفتوحة: %1$s. انقر للتبديل بين التبويبات.</string>
  <!--
    Message announced to the user when tab tray is selected used by a11y services
    The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_open_tab_counter_tab_tray">التبويبات غير الخاصة المفتوحة: %1$s. انقر للتبديل بين التبويبات.</string>
  <!--
    Message announced to the user when the tab counter is selected in private browsing mode.
    Used by a11y services. The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_tab_counter_private">التبويبات الخاصة المفتوحة: %1$s. انقر للانتقال بين التبويبات.</string>
  <!-- Browser menu button that creates a new tab -->
  <string name="mozac_browser_menu_new_tab">تبويب جديد</string>
  <!-- Browser menu button that creates a private tab -->
  <string name="mozac_browser_menu_new_private_tab">تبويب خاص جديد</string>
  <!-- Browser menu button to close tab. Closes the current session when pressed. -->
  <string name="mozac_close_tab">إغلاق التبويب</string>
  <!-- Menu option to duplicate the current tab -->
  <string name="mozac_ui_tabcounter_duplicate_tab">تكرير التبويب</string>
  <!-- Content description of the tab counter toolbar button -->
  <string name="mozac_tab_counter_content_description">زر ”عدد التبويبات“ في شريط الأدوات.</string>
</resources>
